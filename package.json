{
  "name": "token-refresh",
  "version": "0.2.0",
  "description": "A library for refreshing expired access tokens",
  "funding": "https://github.com/mathieuprog/token-refresh?sponsor=1",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "jest --watchAll"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "author": "Mathieu Decaffmeyer",
  "license": "Apache-2.0",
  "peerDependencies": {
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/jest": "^30.0.0",
    "jest": "~30.0.5",
    "rollup": "^4.45.1",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "bugs": {
    "url": "https://github.com/mathieuprog/token-refresh/issues"
  },
  "homepage": "https://github.com/mathieuprog/token-refresh#readme"
}
